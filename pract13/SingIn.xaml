<Window x:Class="pract13.SingIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract13"
        mc:Ignorable="d"
        Title="SingIn" Height="200" Width="250">
    <StackPanel>
        <Label Content="Login" HorizontalAlignment="Center" FontSize="18"></Label>
        <TextBox x:Name="Login_TB" Background="White" Height="30" ></TextBox>
        <Label Content="Password" HorizontalAlignment="Center" FontSize="18"></Label>
        <PasswordBox x:Name="PasswordBox" PasswordChar="*" Height="30"></PasswordBox>
        <Button Height="30" Content="Sing Up" FontSize="18" Click="Login_Click"></Button>
    </StackPanel>
</Window>
